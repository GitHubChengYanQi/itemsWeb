{
  "hash": true,
  "publicPath": "./",
  "plugins": [
    "build-plugin-fast-refresh",
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "outputPath": "assets",
        "relativeCssPath": "../"
      }
    ],
    [
      "build-plugin-antd",
      {
        "themeConfig": {
          "primary-color": "#416e91"
        }
      }
    ]
  ]
}
